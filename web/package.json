{
    "name": "xcan-angusgm-web",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">= 14.0.0",
        "npm": ">= 8.0.0",
        "yarn": "^1.10.0"
    },
    "scripts": {
        "vite:dev:dev": "vite --mode dev",
        "vite:dev:priv": "vite --mode priv",
        "vite:build:dev": "vite build --mode dev",
        "vite:build:prod": "vite build --mode prod",
        "vite:build:priv": "vite build --mode priv",
        "dev": "vite --mode dev",
        "dev:env": "husky install && node scripts/build.js",
        "dev:dev": "husky install && node ./scripts/build.js -- --env=dev --edition_type=CLOUD_SERVICE",
        "dev:prod": "husky install && node ./scripts/build.js -- --env=prod --edition_type=CLOUD_SERVICE",
        "dev:priv:community": "husky install && node scripts/build.js -- --env=priv --edition_type=COMMUNITY",
        "dev:priv:enterprise": "husky install && node scripts/build.js -- --env=priv --edition_type=ENTERPRISE",
        "dev:priv:datacenter": "husky install && node scripts/build.js -- --env=priv --edition_type=DATACENTER",
        "build:env": "node ./scripts/build.js",
        "build:dev": "node ./scripts/build.js -- --env=dev --edition_type=CLOUD_SERVICE",
        "build:prod": "node ./scripts/build.js -- --env=prod --edition_type=CLOUD_SERVICE",
        "build:priv:community": "node ./scripts/build.js -- --env=priv --edition_type=COMMUNITY",
        "build:priv:enterprise": "node ./scripts/build.js -- --env=priv --edition_type=ENTERPRISE",
        "build:priv:datacenter": "node ./scripts/build.js -- --env=priv --edition_type=DATACENTER",
        "fix:js": "eslint --fix --ext \".ts,.tsx,.js,.jsx,.vue\" --ignore-path .eslintignore src/",
        "fix:style": "stylelint src/**/*.{css,less,vue,html} --ignore-path .stylelintignore --fix",
        "fix": "npm run fix:js && npm run fix:style"
    },
    "dependencies": {
        "@xcan-angus/infra": "^1.0.0",
        "@xcan-angus/quill": "^2.0.3",
        "@xcan-angus/vue-ui": "^1.0.0",
        "ant-design-vue": "^3.2.20",
        "axios": "^1.6.2",
        "dayjs": "^1.11.10",
        "dompurify": "^3.0.9",
        "echarts": "^5.4.3",
        "mavon-editor": "^3.0.1",
        "throttle-debounce": "^5.0.0",
        "viewerjs": "^1.11.6",
        "vue": "3.3.8",
        "vue-clipboard3": "^2.0.0",
        "vue-i18n": "^9.8.0",
        "vue-router": "^4.2.5",
        "vue3-video-play": "1.3.1-beta.6",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/throttle-debounce": "^5.0.2",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "@vue/compiler-sfc": "^3.2.47",
        "cross-env": "^7.0.3",
        "postcss": "^8.4.21",
        "postcss-discard-comments": "^6.0.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-compression": "^0.5.1",
        "vue-tsc": "^1.2.0"
    },
    "optionalDependencies": {
        "@commitlint/cli": "^17.5.0",
        "@commitlint/config-conventional": "^17.4.4",
        "@types/jest": "^29.5.2",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vue/test-utils": "^2.3.2",
        "commitizen": "^4.3.0",
        "compressing": "^1.10.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "eslint-plugin-vue": "^7.20.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-serializer-vue": "^3.1.0",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.1.4",
        "postcss-html": "^1.5.0",
        "stylelint": "^15.10.3",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-property-sort-order-smacss": "^9.1.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-less": "^1.0.6",
        "stylelint-order": "^6.0.3",
        "ts-jest": "^29.1.0",
        "vue-eslint-parser": "^9.1.0"
    }
}
