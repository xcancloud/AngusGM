{
    "name": "angus-gm-web",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">= 14.0.0",
        "npm": ">= 8.0.0",
        "yarn": "^1.10.0"
    },
    "scripts": {
        "vite:dev:dev": "vite --mode dev",
        "vite:dev:priv": "vite --mode priv",
        "vite:build:dev": "vite build --mode dev",
        "vite:build:prod": "vite build --mode prod",
        "vite:build:priv": "vite build --mode priv",
        "dev": "husky install && cross-env mode_env=dev edition_type=CLOUD_SERVICE zip_dist=false node scripts/build-dev.js",
        "dev:priv0": "husky install && cross-env mode_env=priv edition_type=$npm_config_edition_type zip_dist=$npm_config_zip_dist node scripts/build-dev.js",
        "dev:priv": "husky install && cross-env mode_env=priv edition_type=COMMUNITY zip_dist=false node scripts/build-dev.js",
        "dev:priv:enterprise": "husky install && cross-env mode_env=priv edition_type=ENTERPRISE zip_dist=false node scripts/build-dev.js",
        "dev:priv:datacenter": "husky install && cross-env mode_env=priv edition_type=DATACENTER zip_dist=false node scripts/build-dev.js",
        "build:dev": "cross-env mode_env=dev edition_type=CLOUD_SERVICE node ./scripts/build.js",
        "build:prod": "cross-env mode_env=prod edition_type=CLOUD_SERVICE node ./scripts/build.js",
        "build:priv0": "cross-env mode_env=priv edition_type=$npm_config_edition_type node ./scripts/build.js",
        "build:priv": "cross-env mode_env=priv edition_type=COMMUNITY node ./scripts/build.js",
        "build:priv:enterprise": "cross-env mode_env=priv edition_type=ENTERPRISE node ./scripts/build.js",
        "build:priv:datacenter": "cross-env mode_env=priv edition_type=DATACENTER node ./scripts/build.js",
        "fix:js": "eslint --fix --ext \".ts,.tsx,.js,.jsx,.vue\" --ignore-path .eslintignore src/",
        "fix:style": "stylelint src/**/*.{css,less,vue,html} --ignore-path .stylelintignore --fix",
        "fix": "npm run fix:js && npm run fix:style"
    },
    "dependencies": {
        "@xcan-angus/tools": "^1.0.0",
        "@xcan-angus/vue-ui": "^1.0.0",
        "ant-design-vue": "^3.2.16",
        "axios": "^1.3.4",
        "dayjs": "^1.11.7",
        "dompurify": "^3.0.9",
        "echarts": "^5.4.2",
        "mavon-editor": "^3.0.1",
        "throttle-debounce": "^5.0.0",
        "viewerjs": "^1.11.3",
        "vue": "3.3.2",
        "vue-clipboard3": "^2.0.0",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6",
        "vue3-video-play": "1.3.1-beta.6",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/throttle-debounce": "^5.0.2",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "@vue/compiler-sfc": "^3.2.47",
        "cross-env": "^7.0.3",
        "postcss": "^8.4.21",
        "postcss-discard-comments": "^6.0.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-compression": "^0.5.1",
        "vue-tsc": "^1.2.0"
    },
    "optionalDependencies": {
        "@commitlint/cli": "^17.5.0",
        "@commitlint/config-conventional": "^17.4.4",
        "@types/jest": "^29.5.2",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "@vue/test-utils": "^2.3.2",
        "commitizen": "^4.3.0",
        "compressing": "^1.10.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "eslint-plugin-vue": "^7.20.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-serializer-vue": "^3.1.0",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.1.4",
        "postcss-html": "^1.5.0",
        "stylelint": "^15.10.3",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-property-sort-order-smacss": "^9.1.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-less": "^1.0.6",
        "stylelint-order": "^6.0.3",
        "ts-jest": "^29.1.0",
        "vue-eslint-parser": "^9.1.0"
    }
}
